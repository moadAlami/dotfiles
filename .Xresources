!=== Colors ===!

! == Font ==!
*.font:			Mono:pixelsize=16:antialias=True:autohint:true;
Xft.dpi:	96

*.depth:		32

!=== rxvt-unicode  ===!
URxvt.background:	[90]#000000
URxvt.scrollBar:	False	
URxvt.font:			xft:monospace:pixelsize=18
URxvt.boldFont:		xft:monospace:pixelsize=20:weight=bold
URxvt.letterSpace:	0
URxvt.urgentOnBell:	False
URxvt.visualBell:	False
URxvt.mapAlert:		False
URxvt.depth:		32

!! urxvt Extensions:
URxvt.perl-ext-common:	default,matcher,resize-font,url-select,keyboard-select
URxvt.colorUL:			#2fe268
! resize-font
URxvt.resize-font.smaller:	C-j
URxvt.resize-font.bigger:	C-k
! url-select
URxvt.keysym.M-u:			perl:url-select:select_next
URxvt.url-select.launcher:	firefox
UExvt.url-select.underline:	true
! keyboard-select
URxvt.keysym.M-Escape:		perl:keyboard-select:activate
! matcher
URxvt.url-launcher:			firefox	
URxvt.matcher.button:		1

!=== Rofi ===!
rofi.font:		System San Francisco Display 15
rofi.lines:		5
